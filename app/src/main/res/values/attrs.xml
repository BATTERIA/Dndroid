<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlowLayout">
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DragFlowLayout">
        <attr name="move_anim_duration" format="integer"/>
        <attr name="resume_anim_duration" format="integer"/>
        <attr name="vibrate_enable" format="boolean"/>
        <attr name="vibrate_duration" format="integer"/>
    </declare-styleable>
</resources>